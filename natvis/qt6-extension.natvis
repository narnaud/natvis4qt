        <DisplayString>empty</DisplayString>
        <Expand>
            <Item Condition="data.d" Optional="true" Name="[width]">data.d-&gt;w</Item>
            <Item Condition="data.d" Optional="true" Name="[height]">data.d-&gt;h</Item>
        </Expand>
    </Type>

